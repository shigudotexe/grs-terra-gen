num wheat = 3 + randomInt(5);

for (num i = 0; i < wheat; i = i + 1) {
    num x = randomInt(6) - 2;
    num z = randomInt(6) - 2;
    
    num rand = randomInt(100);
    
    if (getBlock(x, -1, z) == "minecraft:grass_block" && getBlock(x, 1, z) == "minecraft:air") {
        if (rand >= 35){
			block(x, 0, z, "minecraft:wheat[age=7]");
		}
		else {
			block(x, 0, z, "minecraft:wheat[age=6]");
		}
    }
}